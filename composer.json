{
    "require": {
        "php":                           ">=5.4.0",
        "silex/silex":                   "1.0.*",
        "twig/twig":                     "1.*",
        "symfony/http-foundation":       "2.2.*",
        "symfony/http-kernel":           "2.2.*",
        "symfony/twig-bridge":           "2.2.*",
        "symfony/property-access":       "2.2.*",
        "symfony/yaml":                  "2.2.*",
        "igorw/config-service-provider": "1.2.*",
        "nicl/silex-markdown":           "1.0.*",
        "jtreminio/vagrant-puppet-lamp": "1.*@dev",
        "frastel/puphpeteer-installer":  "dev-master",

        "example42/puppet-apache":          "dev-master",
        "example42/puppi":                  "dev-master",
        "frastel/puppet-nginx":             "dev-master",
        "frastel/puppet-phpmyadmin":        "dev-master",
        "frastel/puppet-symfony":           "dev-master",
        "puphpet/puppet-git":               "dev-master",
        "puphpet/puppet-mysql":             "dev-master",
        "puphpet/puppet-php":               "dev-master",
        "puphpet/puppet-xdebug":            "dev-master",
        "puppetlabs/puppetlabs-apt":        "1.1.0",
        "puppetlabs/puppetlabs-postgresql": "2.2.1",
        "puppetlabs/puppetlabs-stdlib":     "v2.2.1"
    },
    "require-dev": {
        "phpunit/phpunit":                           "3.8.*@dev",
        "whatthejeff/nyancat-phpunit-resultprinter": "1.1.x-dev",
        "symfony/browser-kit":                       "2.2.*",
        "symfony/css-selector":                      "2.2.*"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-0": {
            "Puphpet": "src/"
        }
    },
    "scripts": {
        "post-install-cmd": "Frastel\\Puphpeteer\\Installer::build",
        "post-update-cmd": "Frastel\\Puphpeteer\\Installer::build"
    },
    "extra": {
        "puppet-modules-dir": "puppet/modules",
        "puppet-modules": {
            "example42/puppet-apache":          "apache",
            "example42/puppi":                  "puppi",
            "frastel/puppet-nginx":             "nginx",
            "frastel/puppet-phpmyadmin":        "phpmyadmin",
            "frastel/puppet-symfony":           "symfony",
            "puphpet/puppet-git":               "git",
            "puphpet/puppet-mysql":             "mysql",
            "puphpet/puppet-php":               "php",
            "puphpet/puppet-xdebug":            "xdebug",
            "puppetlabs/puppetlabs-apt":        "apt",
            "puppetlabs/puppetlabs-postgresql": "postgresql",
            "puppetlabs/puppetlabs-stdlib":     "stdlib"
        }
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "example42/puppet-apache",
                "version": "master",
                "source": {
                    "url": "git://github.com/example42/puppet-apache.git",
                    "type": "git",
                    "reference": "master"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "example42/puppi",
                "version": "master",
                "source": {
                    "url": "git://github.com/example42/puppi.git",
                    "type": "git",
                    "reference": "master"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "frastel/puppet-nginx",
                "version": "master",
                "source": {
                    "url": "git://github.com/frastel/puppet-nginx.git",
                    "type": "git",
                    "reference": "master"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "frastel/puppet-phpmyadmin",
                "version": "master",
                "source": {
                    "url": "git://github.com/frastel/puppet-phpmyadmin.git",
                    "type": "git",
                    "reference": "master"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "frastel/puppet-symfony",
                "version": "master",
                "source": {
                    "url": "git://github.com/frastel/puppet-symfony.git",
                    "type": "git",
                    "reference": "master"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "puphpet/puppet-git",
                "version": "master",
                "source": {
                    "url": "git://github.com/puphpet/puppet-git.git",
                    "type": "git",
                    "reference": "master"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "puphpet/puppet-mysql",
                "version": "master",
                "source": {
                    "url": "git://github.com/puphpet/puppet-mysql.git",
                    "type": "git",
                    "reference": "master"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "puphpet/puppet-php",
                "version": "master",
                "source": {
                    "url": "git://github.com/puphpet/puppet-php.git",
                    "type": "git",
                    "reference": "master"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "puphpet/puppet-xdebug",
                "version": "master",
                "source": {
                    "url": "git://github.com/puphpet/puppet-xdebug.git",
                    "type": "git",
                    "reference": "master"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "puppetlabs/puppetlabs-apt",
                "version": "1.1.0",
                "source": {
                    "url": "git://github.com/puppetlabs/puppetlabs-apt.git",
                    "type": "git",
                    "reference": "tags/1.1.0"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "puppetlabs/puppetlabs-postgresql",
                "version": "2.2.1",
                "source": {
                    "url": "git://github.com/puppetlabs/puppetlabs-postgresql.git",
                    "type": "git",
                    "reference": "tags/2.2.1"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "puppetlabs/puppetlabs-stdlib",
                "version": "v2.2.1",
                "source": {
                    "url": "git://github.com/puppetlabs/puppetlabs-stdlib.git",
                    "type": "git",
                    "reference": "tags/v2.2.1"
                }
            }
        }
    ]
}
