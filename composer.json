{
    "require": {
        "silex/silex":                   "1.0.*",
        "twig/twig":                     "1.*",
        "symfony/http-foundation":       "2.2.*",
        "symfony/http-kernel":           "2.2.*",
        "symfony/twig-bridge":           "2.2.*",
        "symfony/property-access":       "2.2.*",
        "symfony/yaml":                  "2.2.*",
        "igorw/config-service-provider": "1.2.*",
        "nicl/silex-markdown":           "1.0.*",
        "frastel/puphpeteer-installer":  "dev-master",

        "example42/puppi":                  "dev-master#f65b71b7c2",
        "frastel/puppet-nginx":             "dev-master#264f66faaa",
        "frastel/puppet-phpmyadmin":        "dev-master#dbfcbf6755",
        "frastel/puppet-symfony":           "dev-master#5558c0b0cf",
        "puphpet/puphpet-apache":           "dev-master",
        "puphpet/puphpet-composer":         "dev-master",
        "puphpet/puphpet-git":              "dev-master",
        "puphpet/puphpet-mysql":            "dev-master",
        "puphpet/puphpet-php":              "dev-master",
        "puphpet/puppet-puphpet":           "dev-master",
        "puphpet/puphpet-xdebug":           "dev-master",
        "puppetlabs/puppetlabs-apt":        "1.1.0",
        "puppetlabs/puppetlabs-postgresql": "2.2.1",
        "puppetlabs/puppetlabs-stdlib":     "v2.2.1"
    },
    "require-dev": {
        "phpunit/phpunit":                           "3.8.*@dev",
        "whatthejeff/nyancat-phpunit-resultprinter": "1.1.x-dev",
        "symfony/browser-kit":                       "2.2.*",
        "symfony/css-selector":                      "2.2.*"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-0": {
            "Puphpet": "src/"
        }
    },
    "scripts": {
        "post-install-cmd": "Frastel\\Puphpeteer\\Installer::build",
        "post-update-cmd": "Frastel\\Puphpeteer\\Installer::build"
    },
    "extra": {
        "puppet-modules-dir": "puppet/modules",
        "puppet-modules": {
            "example42/puppi":                  "puppi",
            "frastel/puppet-nginx":             "nginx",
            "frastel/puppet-phpmyadmin":        "phpmyadmin",
            "frastel/puppet-symfony":           "symfony",
            "puphpet/puphpet-apache":           "apache",
            "puphpet/puphpet-composer":         "composer",
            "puphpet/puphpet-git":              "git",
            "puphpet/puphpet-mysql":            "mysql",
            "puphpet/puphpet-php":              "php",
            "puphpet/puppet-puphpet":           "puphpet",
            "puphpet/puphpet-xdebug":           "xdebug",
            "puppetlabs/puppetlabs-apt":        "apt",
            "puppetlabs/puppetlabs-postgresql": "postgresql",
            "puppetlabs/puppetlabs-stdlib":     "stdlib"
        }
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "example42/puppi",
                "version": "master",
                "source": {
                    "url": "git://github.com/example42/puppi.git",
                    "type": "git",
                    "reference": "master"
